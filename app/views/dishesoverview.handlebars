
<div class="col-md-offset-3 col-md-6">
    <ul class="col-md-12">
    {{#each meal}}
        <li class="col-xs-4 col-sm-3 col-md-3 col-lg-2 col-xl-2">
            <div class="blocks">
                <div class="blocks-height"></div>
                <div class="blocks-layer blocks-purple-white">
                    <div class="blocks-text">{{this}}</div>
                </div>
            </div>
        </li>
    {{/each}}
    </ul>
</div>